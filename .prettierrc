{
  "semi": true,
  "singleQuote": false,
  "jsxSingleQuote": false,
  "quoteProps": "as-needed",
  "jsxBracketSameLine": true,
  "arrowParens": "avoid",
  "tabWidth": 2,
  "useTabs": false,
  "printWidth": 120,
  "endOfLine": "auto",
  "trailingComma": "all",
  "bracketSpacing": true,
  "arrowSpacing": true,
  "embeddedLanguageFormatting": "auto",
  "bracketSameLine": false,
  "proseWrap": "always",
  "singleAttributePerLine": true
}
